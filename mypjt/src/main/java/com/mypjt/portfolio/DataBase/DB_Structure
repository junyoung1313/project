create table member(
  mEmail varchar2(30),
  mPassword varchar2(12),
  mNickName varchar2(20),
  mJoinDate timestamp,
  mIsDelete varchar2(1),
  mDeleteDate timestamp
);


create table memberBoard(
  mbNum number,
  mEmail varchar2(30),
  mNickName varchar2(20),
  mbTitle varchar2(200),
  mbContent varchar2(200),
  mbDate timestamp,
  mbHit number,
  mbGoods number,
  mbIsModify varchar2(1),
  mbModifyDate timestamp,
  mbIsDelete varchar2(1),
  mbDeleteDate TIMESTAMP
);

create SEQUENCE memberBoard_SEQ;


