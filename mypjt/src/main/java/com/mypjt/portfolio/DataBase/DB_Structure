create table freeboard(
bNum number,
bName varchar2(200),
bPassword varchar2(200),
bTitle varchar2(200),
bContent varchar2(200),
bDate timestamp,
bHit number,
bGroup number,
bStep number,
bIndent number,
bGood number,
bDel varchar2(1),
bDelDate timestamp,
bModi varchar2(1),
bModifyDate timestamp
);

create SEQUENCE freeboard_seq;

